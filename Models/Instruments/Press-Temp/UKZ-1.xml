<animation>
	<type>material</type>
	<object-name>Aguja1</object-name>
	<object-name>Aguja2</object-name>
	<object-name>Aguja3</object-name>
	<object-name>Fondo</object-name>
	<emission>
		<red>  0.7</red>
		<green>0.7</green>
		<blue> 0.7</blue>
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
	</emission>
</animation>

<animation>
	<type>rotate</type>
	<object-name>Aguja1</object-name>
	<property>/engines/engine/oil-temperature-degf</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>302</ind><dep>120</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
	<center>
		<x-m>0.002</x-m>
		<y-m>0.000</y-m>
		<z-m>0.008</z-m>
	</center>
</animation>
