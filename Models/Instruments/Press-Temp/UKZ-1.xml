<animation>
	<type>rotate</type>
	<object-name>Aguja3</object-name>
	<property>engines/engine/oil-pressure-psi</property>
	<interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>217</ind><dep>120</dep></entry>
	</interpolation>
	<axis>
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
	<center>
		<x-m> 0.002</x-m>
		<y-m> 0.020</y-m>
		<z-m>-0.0095</z-m>
	</center>
