<?xml version="1.0"?>

<PropertyList>

	<path>MPress.ac</path>

<animation>
	<object-name>Fondo</object-name>
	<object-name>Base</object-name>
	<object-name>Aguja</object-name>
	<object-name>Cristal</object-name>
</animation>

<animation>
	<type>noshadow</type>
	<object-name>Cristal</object-name>
</animation>
<!--
<animation>
	<type>shader</type>
	<shader>chrome</shader>
	<texture>glass.png</texture>
</animation>
